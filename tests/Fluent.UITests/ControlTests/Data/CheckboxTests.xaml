                </ut:TestDictionary>

                <ut:TestDictionary Name="CheckedDisabled">
                    <!--Not defined in WPF Xaml-->
                    <SolidColorBrush x:Key="CheckBoxForeground" Color="{StaticResource TextFillColorDisabled}" />
                    <!-- __Content border icon properties-->
                    <SolidColorBrush x:Key="CheckBoxBackground_Fill" Color="{StaticResource AccentFillColorDisabled}" />
                    <!-- Checkbox : stroke border Properties -->
                    <SolidColorBrush x:Key="CheckBoxBorderBrush_Stroke" Color="{StaticResource ControlStrongStrokeColorDisabled}" />
                    <!-- Checkbox: Textblock-->
                    <SolidColorBrush x:Key="CheckBoxTextblock_Foreground" Color="{StaticResource TextFillColorDisabled}" />

                </ut:TestDictionary>

                <ut:TestDictionary Name="IndeterminateDisabled">

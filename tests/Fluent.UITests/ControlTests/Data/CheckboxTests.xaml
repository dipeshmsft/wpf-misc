                <ut:TestDictionary Name="CustomizedBrush">
                    <SolidColorBrush x:Key="CheckBoxBackground" Color="Aqua" />
                    <!--<SolidColorBrush x:Key="CheckBoxBackground_Fill" Color="Black" />-->
                    <SolidColorBrush x:Key="CheckBoxForeground" Color="Maroon" />
                    <SolidColorBrush x:Key="CheckBoxBorderBrush" Color="Green" />
                </ut:TestDictionary>
                
                <ut:TestDictionary Name="CustomCheckbox">
                    <Thickness x:Key="CheckBox_Thickness">4</Thickness>
                    <Thickness x:Key="CheckBox_Padding">8,5,10,10</Thickness>
                    <HorizontalAlignment x:Key="Checkbox_HorizontalAlignment">Right</HorizontalAlignment>
                    <VerticalAlignment x:Key="Checkbox_VerticalAlignment">Bottom</VerticalAlignment>
                    <!--<FontWeight x:Key="CheckBox_Fontweight">Normal</FontWeight>-->
                    <HorizontalAlignment x:Key="Checkbox_HorizontalContentAlignment">Right</HorizontalAlignment>
                    <VerticalAlignment x:Key="Checkbox_VerticalContentAlignment">Bottom</VerticalAlignment>
                    <sys:Boolean x:Key="Checkbox_IsTabStop">False</sys:Boolean>
                    <sys:Double x:Key="CheckBox_Height">40</sys:Double>
                    <sys:Double x:Key="CheckBox_MinWidth">150</sys:Double>
                    <!--<CornerRadius x:Key="CheckBox_CornerRadius">10,10,10,10</CornerRadius>-->
                    <!--<Cursor x:Key="CheckBox_Cursor">Hand</Cursor>-->
                    <!-- Checkbox : ContentPresenter Properties -->
                    <sys:Boolean x:Key="ContentPresenter_RecognizesAccessKey">True</sys:Boolean>
                    <HorizontalAlignment x:Key="ContentPresenter_HorizontalAlignment">Right</HorizontalAlignment>
                    <VerticalAlignment x:Key="ContentPresenter_VerticalAlignment">Bottom</VerticalAlignment>
                    <Thickness x:Key="ContentPresenter_Margin">8,5,10,10</Thickness>
                </ut:TestDictionary>

            </ut:TestDictionary.MergedDictionaries>
        </ut:TestDictionary>

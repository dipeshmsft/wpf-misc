<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=System.Runtime">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary x:Name="Button_Normal_Light">
            <HorizontalAlignment x:Key="HorizontalAlignment">Stretch</HorizontalAlignment>
            <VerticalAlignment x:Key="VerticalAlignment">Center</VerticalAlignment>
            <sys:Boolean x:Key="IsFocusable">True</sys:Boolean>
            <SolidColorBrush x:Key="Background" Color="" />
        </ResourceDictionary>
        <ResourceDictionary x:Name="Button_Disabled">
            
        </ResourceDictionary>
        
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>